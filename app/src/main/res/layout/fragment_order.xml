<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presenters.ui.fragment.order.OrderFragment">

    <ImageView
        android:id="@+id/img_page"
        android:layout_width="219dp"
        android:layout_height="166dp"
        android:importantForAccessibility="no"
        android:src="@drawable/not_found"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.38" />

    <TextView
        android:id="@+id/message_page"
        style="@style/Theme.GojekClone.Text.Headline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/message_not_found"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@id/img_page"
        app:layout_constraintStart_toStartOf="@id/img_page"
        app:layout_constraintTop_toBottomOf="@id/img_page" />

</androidx.constraintlayout.widget.ConstraintLayout>